<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="com.word.mapper.WordMapper">

    <select id="find" resultType="Word">
        <![CDATA[ select * from sys_user_t]]>
    </select>

    <update id="upd" parameterType="Word">

        <![CDATA[
        update sys_user_T set USER_NAME=#{userName} where login_name=#{loginName}
        ]]>
    </update>

</mapper>